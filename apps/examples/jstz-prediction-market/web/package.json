    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@uidotdev/usehooks": "^2.4.1",
    "better-auth": "^1.4.9",
    "cva": "1.0.0-beta.4",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
